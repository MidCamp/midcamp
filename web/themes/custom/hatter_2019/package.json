{
  "name": "hatter_2019",
  "version": "1.0.0",
  "description": "Node dependencies for Hatter 2019 theme",
  "scripts": {
    "postinstall": "rimraf dist && rimraf js && rimraf svg && vendor-copy"
  },
  "private": true,
  "dependencies": {
    "@backlineint/hatter": "^1.0.6",
    "rimraf": "^3.0.0",
    "vendor-copy": "^2.0.0"
  },
  "vendorCopy": [
    {
      "from": "node_modules/@backlineint/hatter/dist",
      "to": "dist"
    },
    {
      "from": "node_modules/@backlineint/hatter/source/svg",
      "to": "svg"
    },
    {
      "from": "node_modules/@backlineint/hatter/source/js",
      "to": "js"
    }
  ]
}
